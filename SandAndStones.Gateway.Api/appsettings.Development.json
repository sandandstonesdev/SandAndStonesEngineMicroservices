{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy" : {
    "Routes": {
      "asset-route": {
        "ClusterId": "asset-cluster",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "asset-cluster": {
        "Destinations": {
          "api-destination": {
            "Address": "https://sand-and-stones-microservices-0001.azurewebsites.net"
          }
        }
      }
    }
  }
}
